// Leetcode 26
// Easy

class Solution {
    public int removeDuplicates(int[] nums) {
        int count=0;
        for(int i=0;i<nums.length;i++){
            if(i<nums.length-1 && nums[i]!=nums[i+1]){
                nums[count]=nums[i];
                count++;
            }
            else continue;
        }
        nums[count]=nums[nums.length-1];
        count++;
        return count;
    }
}
